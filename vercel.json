{
  "buildCommand": "cd frontend && ./build.sh",
  "outputDirectory": "frontend/dist",
  "installCommand": "cd frontend && pnpm install --frozen-lockfile --prefer-offline",
  "framework": null,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "frontend/dist/**/*.html": {
      "maxDuration": 30
    }
  }
}